          )
          .addSeparator()
          .addItem("üìò –û—Ç—á—ë—Ç –ø–æ —Ç–æ–≤–∞—Ä–∞–º", "reportProducts")
          .addItem("üì∑ –§–æ—Ç–æ —Ç–æ–≤–∞—Ä–æ–≤", "productsPhoto")
          .addItem("üíµ –¶–µ–Ω—ã (–≤—Å—ë)", "pricesFull")
          .addSeparator()
          .addSubMenu(
            ui
              .createMenu("‚öôÔ∏è –ù–∞—Å—Ç—Ä–æ–π–∫–∏")
              .addItem("üîë Seller API", "ozonSetSellerAccess")
          )
          .addToUi();
      },
      chat() {
        var ui = SpreadsheetApp.getUi();
        ui.createMenu(`${this.title}`)
          .addItem("–°–ø–∏—Å–æ–∫ —á–∞—Ç–æ–≤", "chatList")
          .addItem("–ò—Å—Ç–æ—Ä–∏—è —á–∞—Ç–∞", "chatHistory")
          .addItem("–û—Ç–ø—Ä–∞–≤–∏—Ç—å —Å–æ–æ–±—â–µ–Ω–∏–µ –≤ —á–∞—Ç", "chatSendMessage")
          .addSubMenu(
            ui
              .createMenu("‚öôÔ∏è –ù–∞—Å—Ç—Ä–æ–π–∫–∏")
              .addItem("üîë Seller API", "ozonSetSellerAccess")
              .addSeparator()
              .addItem("üÖ∞Ô∏è –¢–µ–∫—É—â–∞—è –≤–µ—Ä—Å–∏—è", "Ozon.ozonLibraryVersion")
          )
          .addToUi();
      },
      reviews() {
        var ui = SpreadsheetApp.getUi();
        ui.createMenu(`${this.title}`)
          .addItem("–û—Ç–∑—ã–≤–æ–≤ –ø–æ —Å—Ç–∞—Ç—É—Å–∞–º", "rewiewsCount")
          .addItem("–°–ø–∏—Å–æ–∫ –æ—Ç–∑—ã–≤–æ–≤", "rewiewsList")

          .addItem("–û—Ç–ø—Ä–∞–≤–∏—Ç—å —Å–æ–æ–±—â–µ–Ω–∏–µ –≤ —á–∞—Ç", "chatSendMessage")
          .addSubMenu(
            ui
              .createMenu("‚öôÔ∏è –ù–∞—Å—Ç—Ä–æ–π–∫–∏")
              .addItem("üîë Seller API", "ozonSetSellerAccess")
              .addSeparator()
              .addItem("üÖ∞Ô∏è –¢–µ–∫—É—â–∞—è –≤–µ—Ä—Å–∏—è", "Ozon.ozonLibraryVersion")
          )
          .addToUi();
      },
      moysklad(menu, ui) {
        return menu
          .addSeparator()
          .addSubMenu(
            ui
              .createMenu("–ú–æ–π–°–∫–ª–∞–¥")
              .addItem("üìÑ –ê—Å—Å–æ—Ä—Ç–∏–º–Ω–µ—Ç", "moyskladAssortment")
              .addItem("üì¶ –û—Å—Ç–∞—Ç–∫–∏", "moyskladStocks")
              .addItem("üì¶ –û—Å—Ç–∞—Ç–∫–∏ –ø–æ —Å–∫–ª–∞–¥–∞–º", "moyskladStocksByStores")
              .addSeparator()
              .addSubMenu(
                ui
                  .createMenu("‚öôÔ∏è –ù–∞—Å—Ç—Ä–æ–π–∫–∏")
                  .addItem("üîë –¢–æ–∫–µ–Ω", "moyskladSetToken")
              )
              .addSeparator()
              .addSubMenu(
                ui
                  .createMenu("‚è≥ –ê–≤—Ç–æ–æ–±–Ω–æ–≤–ª–µ–Ω–∏–µ")
                  .addItem("–ê—Å—Å–æ—Ä—Ç–∏–º–Ω–µ—Ç", "Ozon.triggerMoysklad1")
                  .addItem("–û—Å—Ç–∞—Ç–∫–∏", "Ozon.triggerMoysklad2")
                  .addItem("–û—Å—Ç–∞—Ç–∫–∏ –ø–æ —Å–∫–ª–∞–¥–∞–º", "Ozon.triggerMoysklad3")
              )
          );
      },
    },
    func: {
      start: () => {
        return new Date().getTime();
      },

      end: (s) => {
        const e = new Date().getTime();
        return e - s;
      },

      endScript: ({
        script,
        start,
        error = null,
        sheet,
        func,
        clear = false,
        emptyCols = true,
        emptyRows = true,
        setFilter = true,
        autoResize = false,
        removeDuplicates = false,
        message,
        messageType = 1,
        scipError = false,
      }) => {
        let e;
        try {
          if (sheet) {
            const filter = sheet.getFilter();
            const lastRow = sheet.getLastRow();
            const lastColumn = sheet.getLastColumn();
            const maxRows = sheet.getMaxRows();
            const maxColumns = sheet.getMaxColumns();
            const sheetName = sheet.getName();

            if (filter) filter.remove();

            if (clear) sheet.clear();

            if (func) func();

            // SpreadsheetApp.flush();

            if (removeDuplicates) removeDuplicatesByColNative(sheet, 1);

            if (emptyCols && emptyRows && lastRow > 1 && lastColumn > 1) {
              cropToRange(sheet.getDataRange());
            }

            if (
              emptyCols &&
              !emptyRows &&
              maxColumns > lastColumn &&
              lastColumn > 1
            ) {
              removeEmptyColumns(sheet);
            }

            if (emptyRows && !emptyCols && maxRows > lastRow && lastRow > 1)
              removeEmptyRows(sheet);

            if (setFilter && lastRow > 1)
              sheet.getRange(1, 1, lastRow, maxColumns).createFilter();

            if (autoResize) sheet.autoResizeColumns(1, maxColumns);

            if (!message && messageType === 1)
              ss.toast(`[INFO] ${sheetName} –æ–±–Ω–æ–≤–∏–ª–∏`, "‚úÖ SUCCESS", 7);

            if (message && messageType === 2)
              ss.toast(`[WARNING] ${message} ${sheetName}`, "‚ö†Ô∏è ATTENTION", 7);
          }

          if (message && messageType === 1)
            ss.toast(`[INFO] ${message}`, "‚úÖ SUCCESS", 7);

          if (!sheet && message && messageType === 2)
            ss.toast(`[INFO] ${message}`, "‚ö†Ô∏è ATTENTION", 7);

          if (error && !scipError) {
            console.log(error.stack);
            SpreadsheetApp.getUi().alert(
              `‚ö†Ô∏è ATTENTION`,
              error.stack,
              SpreadsheetApp.getUi().ButtonSet.OK
            );
          }
        } catch (e) {
          return e;
        } finally {
          const time_end = CONFIG.func.end(start);
          const err = error ? error : e;

          // if (err) console.log(err?.stack);

          const stopPattern =
            /Unauthorized|Forbidden|Not Found|–í —Ä–µ–∑—É–ª—å—Ç–∞—Ç–µ —ç—Ç–æ–≥–æ –¥–µ–π—Å—Ç–≤–∏—è –∫–æ–ª–∏—á–µ—Å—Ç–≤–æ —è—á–µ–µ–∫ –≤ –∫–Ω–∏–≥–µ –ø—Ä–µ–≤—ã—Å–∏—Ç –º–∞–∫—Å–∏–º–∞–ª—å–Ω–æ –¥–æ–ø—É—Å—Ç–∏–º–æ–µ (10000000)|Service Spreadsheets failed while accessing document with id|–°–ª—É–∂–±–∞ –±—ã–ª–∞ –≤—ã–∑–≤–∞–Ω–∞ —Å–ª–∏—à–∫–æ–º –º–Ω–æ–≥–æ —Ä–∞–∑ –∑–∞ –¥–µ–Ω—å: urlfetch./i;

          if (err && stopPattern.test(err?.message)) {
            const user = ss.getOwner().getEmail();

            // console.log(
            //   Session.getActiveUser().getEmail(),
            //   ss.getOwner().getEmail()
            // );

            const subject = "‚ö†Ô∏è –û—à–∏–±–∫–∞ –≤—ã–ø–æ–ª–Ω–µ–Ω–∏—è —Å–∫—Ä–∏–ø—Ç–∞ Ozon";
            const autoanswer =
              "<br><hr><h3><em><strong>–ü–æ–∂–∞–ª—É–π—Å—Ç–∞, –Ω–µ –æ—Ç–≤–µ—á–∞–π—Ç–µ –Ω–∞ —ç—Ç–æ –ø–∏—Å—å–º–æ, –æ–Ω–æ —Å–≥–µ–Ω–µ—Ä–∏—Ä–æ–≤–∞–Ω–æ –∞–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–∏.</strong></em></h3>";

            const message = {
              to: user,
              name: "Automatic Emailer Script",
              subject,
              htmlBody:
                "<b>–°–æ–æ–±—â–µ–Ω–∏ –æ–± –æ—à–∏–±–∫–µ.</b><br><br>–§–∞–π–ª: " +
                ss.getUrl() +
                "<br><br>–û—à–∏–±–∫–∞:\n" +
                err.stack +
                autoanswer,
            };

            return MailApp.sendEmail(message);
          } else {
            return loock(script, time_end, err);
          }
        }
      },
    },
  };
})();

function emtyMenu() {
  return;
}

function setStorage({
  service,
  serviceName = "Ozon",
  serviceType = "getScriptProperties",
}) {
  if (!context?.storage)
    return new StorageService({
      service,
      serviceName,
      serviceType,
    });
}

function init(tableContext, options = {}) {
  if (!tableContext) {
    tableContext = context?.this_;
  }

  const service = tableContext?.PropertiesService;
  Object.assign(context, { ...options, this_: tableContext, service });

  // –û–±—ä–µ–¥–∏–Ω—è–µ–º –¥–µ—Ñ–æ–ª—Ç–Ω—ã–µ –∑–Ω–∞—á–µ–Ω–∏—è —Å –ø–µ—Ä–µ–¥–∞–Ω–Ω—ã–º–∏
  const defaultOptions = {
    serviceName: "Ozon",
    serviceType: "getScriptProperties",
  };

  const finalOptions = { ...defaultOptions, ...options };

  if (!context?.storage) {
    context.storage = new StorageService({
      service,
      ...finalOptions,
    });
  }

  return context;
}

/**
 * Sets the library version in a specified range on a spreadsheet.
 *
 * @return {void} This function does not return anything.
 */
function ozonLibraryVersion() {
  const { libraryVersion, libraryVersionRange, sheetSettings } =
    CONFIG.settings;
  setSheet(sheetSettings)
    .getRange(libraryVersionRange)
    .setValue(`–í–µ—Ä—Å–∏—è: ${libraryVersion}`);
}

