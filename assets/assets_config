// ####################################################################################################################################

const dateTimeNow = CONFIG.settings.time.now;
const tzoffset = new Date().getTimezoneOffset() * 60000; // offset in milliseconds

// const filterDateFromStartYearMsc = new Date(
//   new Date(dateTimeNow.getFullYear(), 0, 1, 0, 0, 0) - tzoffset
// ).toISOString();

const filterDateFromStartYearMsc = new Date(
  Date.UTC(dateTimeNow.getFullYear(), 0, 1)
).toISOString();

// const filterDateFromStartYear = new Date(
//   new Date(dateTimeNow.getFullYear(), 0, 1, 0, 0, 0) // - tzoffset
// ).toISOString();
// .slice(0, -1);

const filterDateFromStartYear = new Date(
  Date.UTC(dateTimeNow.getFullYear(), 0, 1) + tzoffset
).toISOString();

const filterDateEndStartYear = new Date(
  new Date(dateTimeNow.getFullYear(), 11, 31, 23, 59, 59, 999)
).toISOString();

const filterDateFromStartMonth = new Date(
  new Date(dateTimeNow.getFullYear(), dateTimeNow.getMonth(), 1, 0, 0, 0)
).toISOString();

const filterDateFromEndMonth = new Date(
  new Date(dateTimeNow.getFullYear(), dateTimeNow.getMonth() + 1, 0, 0, 0, 0, 0)
).toISOString();

const filterDateFromStartLastMonth = new Date(
  new Date(dateTimeNow.getFullYear(), dateTimeNow.getMonth() - 1, 1, 0, 0, 0, 0)
).toISOString();

const filterDateCutoffTo = new Date(
  new Date(
    dateTimeNow.getFullYear(),
    dateTimeNow.getMonth(),
    dateTimeNow.getDate() + 1,
    13,
    0,
    0,
    0
  )
).toISOString(); // 13:00 –ú–°–ö UTC-0

const filterDateCutoffToMsc = new Date(
  new Date(
    dateTimeNow.getFullYear(),
    dateTimeNow.getMonth(),
    dateTimeNow.getDate() + 1,
    13,
    0,
    0,
    0
  ) - tzoffset
).toISOString(); // 13:00 –ú–°–ö UTC+3

const filterDateYesterdayStart = new Date(
  new Date(
    dateTimeNow.getFullYear(),
    dateTimeNow.getMonth(),
    dateTimeNow.getDate() - 1,
    0,
    0,
    0,
    0
  )
).toISOString();

const filterDateYesterdayEnd = new Date(
  new Date(
    dateTimeNow.getFullYear(),
    dateTimeNow.getMonth(),
    dateTimeNow.getDate() - 1,
    23,
    59,
    59,
    999
  )
).toISOString();

const filterDateTwoDaysAgoEnd = new Date(
  new Date(
    dateTimeNow.getFullYear(),
    dateTimeNow.getMonth(),
    dateTimeNow.getDate() - 2,
    23,
    59,
    59,
    999
  )
).toISOString();

const filterDateToday = new Date(new Date().setHours(0, 0, 0, 0)).toISOString();
const filterDateTodayEnd = new Date(
  new Date().setHours(23, 59, 59, 999)
).toISOString();

const filterDateTomorrow = new Date(
  new Date(
    dateTimeNow.getFullYear(),
    dateTimeNow.getMonth(),
    dateTimeNow.getDate() + 1,
    23,
    59,
    59,
    999
  )
).toISOString();

const filterDateToNow = new Date(Date.now()).toISOString();

const countMinusDay = 90; // –ö–æ–ª-–≤–æ –¥–Ω–µ–π –¥–ª—è –æ–±–Ω–æ–≤–ª–µ–Ω–∏—è
const minusDay = new Date(new Date() - countMinusDay * 24 * 60 * 60 * 1000); // –ú–∏–Ω—É—Å –¥–Ω–µ–π
const filterDatefinanceTransactionListFromOld = new Date(
  new Date(
    minusDay.getFullYear(),
    minusDay.getMonth(),
    minusDay.getDate(),
    0,
    0,
    0
  )
).toISOString();

const filterDatefinanceTransactionListFrom2023 = new Date(
  new Date(2023, 0, 1, 0, 0, 0)
).toISOString();

const filterDatefinanceTransactionListFrom = new Date(
  new Date(dateTimeNow.getFullYear(), dateTimeNow.getMonth(), 1, 0, 0, 0, 0) -
    tzoffset
).toISOString();

const filterDatefinanceTransactionListFromEndMonth = new Date(
  new Date(dateTimeNow.getFullYear(), dateTimeNow.getMonth() + 1, 0, 0, 0, 0) -
    tzoffset
).toISOString();

function filterDateFromIf({
  sh,
  range,
  days = 1,
  offsettz = false,
  iso = true,
  yyyymmdd = false,
}) {
  const lastRow = sh && sh.getLastRow() > 0 ? sh.getLastRow() : null;

  if (lastRow) {
    const dates = sh
      .getRange(range + lastRow)
      .getValues()
      .flat();

    if (dates && dates.length) {
      // const sorted = dates
      //   .slice()
      //   .sort((a, b) => new Date(a) - new Date(b))
      //   .pop();

      const sorted = dates.sort((a, b) => new Date(a) - new Date(b)).pop();
      console.log(sorted);

      const minus = days ? days * 24 * 60 * 60 * 1000 : 0;
      const tz = offsettz ? tzoffset : 0;
      const dateFrom = new Date(
        new Date(sorted).setHours(0, 0, 0, 0) - minus - tz
      );
      const dateToEnd = new Date(
        new Date(sorted).setHours(23, 59, 59, 999) - minus - tz
      );

      const result = {
        dateFrom: iso ? dateFrom.toISOString() : dateFrom,
        dateTo: filterDateYesterdayEnd,
        dateToEnd: iso ? dateToEnd.toISOString() : dateToEnd,
      };

      if (yyyymmdd) {
        result.dateFrom = Utilities.formatDate(
          dateFrom,
          "GMT+03:00",
          "yyyy-MM-dd"
        );
        result.dateToEnd = Utilities.formatDate(
          dateToEnd,
          "GMT+03:00",
          "yyyy-MM-dd"
        );
      }
      return result;
    }
  }
}

