        const { moysklad } = options;
        var ui = SpreadsheetApp.getUi();
        var menu = ui
          .createMenu(`${this.title}`)
          .addItem("–°–ø–∏—Å–æ–∫ –æ—Ç–ø—Ä–∞–≤–ª–µ–Ω–∏–π FBO (–∑–∞–∫–∞–∑—ã)", "fboPostingList")
